{
  "extends": ["eslint-config-airbnb-base", "prettier"],
  "rules": {
    "no-console": "off",
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "singleQuote": true
      }
    ]
  },
  "env": {
    "browser": true,
    "node": true
  },
  "plugins": ["prettier"]
}
